{
    "views": {
        "target_view": {
            "resource_id": ".*title",
            "class": ".*TextView",
            "text": "Personal API key"
        }
    },
    "states": {
        "target_state": {
            "views": ["target_view"]
        }
    },
    "operations": {
        "target_operation": [
            {
                "event_type": "touch",
                "target_view": "target_view"
            }
        ]
    },
    "main": {
        "target_state": ["target_operation"]
    }
}