{
    "views": {
        "other_view2": {
            "resource_id": "ca.momi.lift:id/textView",
            "class": "android.widget.TextView",
            "text": "Program"
        },
        "other_view1": {
            "class": "android.widget.TextView",
            "text": "Lift"
        },
        "target_view": {
            "resource_id": "ca.momi.lift:id/setProgram",
            "class": "android.widget.Button",
            "text": "SET PROGRAM (WILL RESTART APP)"
        }
    },
    "states": {
        "target_state": {
            "views": ["target_view", "other_view1", "other_view2"]
        }
    },
    "operations": {
        "target_operation": [
            {
                "event_type": "touch",
                "target_view": "target_view"
            }
        ]
    },
    "main": {
        "target_state": ["target_operation"]
    }
}
